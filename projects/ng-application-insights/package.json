{
  "name": "@wizsolucoes/ng-application-insights",
  "version": "0.0.1",
  "scripts": {
    "build": "../../node_modules/.bin/tsc -p tsconfig.schematics.json",
    "copy:schemas": "cp --parents schematics/*/schema.json ../../dist/ng-application-insights/",
    "copy:files": "cp --parents -p schematics/*/files/** ../../dist/ng-application-insights/",
    "copy:collection": "cp schematics/collection.json ../../dist/ng-application-insights/schematics/collection.json",
    "postbuild": "npm run copy:schemas && npm run copy:files && npm run copy:collection"
  },
  "peerDependencies": {
    "@angular/common": "^10.0.14",
    "@angular/core": "^10.0.14",
    "@microsoft/applicationinsights-web": "^2.5.9"
  },
  "dependencies": {
    "tslib": "^2.0.0"
  },
  "devDependencies": {
    "@microsoft/applicationinsights-web": "^2.5.9"
  },
  "schematics": "../ng-application-insights-schematic/src/collection.json",
  "ng-add": {
    "save": "dependencies"
  }
}
